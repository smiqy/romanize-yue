{
    "manifest_version": 2,
    "name": "yue-romanize",
    "description": "Converts Chinese characters in a webpage to my original romanization at the press of a button.",
    "version": "1.0",
    "browser_action": {},
    "background": {
        "scripts": [ "jquery.min.js", "charmap.js", "background.js" ]
    },
    "content_scripts": [{
        "css": [ "styles.css" ]
    }],
    "permissions": [
        "tabs", "*://*/*"
    ]
}